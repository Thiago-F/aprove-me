  "scripts": {
    "build": "npx nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:build:prod": "npm run prisma:deploy && npx prisma generate && npm run prisma:seed",
    "prisma:build": "npm run prisma:migrate && npm run prisma:generate && npm run prisma:seed",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate dev",
    "prisma:seed": "npx prisma db seed",
    "prisma:deploy": "npx prisma migrate deploy",
    "pretest": "jest --clearCache",
    "test": "jest --silent --noStackTrace --runInBand",
    "test:verbose": "jest --runInBand",
    "test:unit": "npm test -- --watch --coverage",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.0",
